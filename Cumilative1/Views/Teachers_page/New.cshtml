

<div id="containershow">
    <nav>
        <a class="navlink" href="/Teachers_Page/List">Back To Teachers</a>
    </nav>
    <h2>New Teacher</h2>
    <form method="POST" action="/Teachers_Page/Create">
        <div class="inputrow">
            <label for="Teacher_id">Teacher Id:</label>
            <input type="number" id="Teacher_id" name="Teacher_id" />
        </div>

        <div class="inputrow">
            <label for="TeacherFirstName">Teacher First Name:</label>
            <input type="text" id="TeacherFirstName" name="TeacherFirstName" />
        </div>
        <div class="inputrow">
            <label for="TeacherLastName">Teacher Last Name:</label>
            <input type="text" id="TeacherLastName" name="TeacherLastName" />
        </div>

        <div class="inputrow">
            <label for="EmpNumber">Teacher Employee Number:</label>
            <input type="text" id="EmpNumber" name="EmpNumber" />
        </div>
        <div class="inputrow">
            <label for="HireDate">Teacher Hire Date:</label>
            <input type="datetime" id="HireDate" name="HireDate" />
        </div>
        <div class="inputrow">
            <label for="Salary">Teacher Salary:</label>
            <input type="number" id="Salary" name="Salary"/>

        </div>

        <div class="inputrow">
            <input type="submit" value="Create Teacher" />
        </div>
    </form>
</div>